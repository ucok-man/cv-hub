generator client {
  provider = "prisma-client"
  output   = "./gen"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Resume {
  id        String   @id @default(cuid())
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  jobTitle       String
  jobDescription String
  images         String[]

  feedback Json
}
